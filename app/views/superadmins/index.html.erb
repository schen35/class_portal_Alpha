<div class="container">
  <div class="row col-md-4 col-md-offset-4" style="left: 0%; top: 70px;">
    <p id="notice"><%= notice %></p>

    <h1>Manage Users</h1>

    <table  class="table table-condensed" border="4">
      <thead>
      <tr>
        <th>E-Mail</th>
        <th>Name</th>
        <th>Role</th>
        <th colspan="3"></th>
      </tr>
      </thead>

      <tbody>

          <% @users.each do |user| %>
          <tr>
           <% if (user.role != "superadmin") %>
            <td><%= user.email %></td>
            <td><%= user.name %></td>
            <td><%= user.role %></td>

            <td><%= link_to 'Delete', user , method: :delete %></td>
            <% end %>
            </tr>
      <% end %>
      </tbody>
    </table>

    <br>

    <%= link_to 'New User', new_user_path %>

  </div>
</div>
